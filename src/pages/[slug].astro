---
import Home from '../components/ContentHome.astro'
import Carreras from '../components/ContentCarreras.astro'
import Vacunatorio from '../components/ContentVacunatorio.astro'

export async function getStaticPaths() {
    return [
        { params: { slug: "Carreras" }, props: { type: "Carreras" } },
        { params: { slug: "Vacunatorio" }, props: { type: "Vacunatorio" } },
    ];
}

const { type } = Astro.props;
---



{type === "Carreras" ? <Carreras /> : console.log("")}
{type === "Vacunatorio" ? <Vacunatorio /> : console.log("")}
{typeof type ===  "undefined" ? <Home /> : console.log("")}

